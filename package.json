{
  "name": "todolist",
  "version": "1.0.0",
  "description": "todolist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptoner/todolist.git"
  },
  "scripts": {
    "serve": "http-server ./www/ -o -c 1 -a localhost -p 8081",
    "build": "./build.sh",
    "watch": "watchify --debug -s Todo -t [ babelify --presets [ @babel/preset-env ] --plugins [ @babel/plugin-proposal-class-properties ] ] ./ts/index.ts -p [ tsify  ] -o ./www/js/todo.js -v"
  },
  "keywords": [
    "framework7",
    "template",
    "ios",
    "android",
    "mobile",
    "app",
    "cordova",
    "phonegap"
  ],
  "author": "Patrick Toner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ptoner/todlist/issues"
  },
  "homepage": "https://github.com/ptoner/todolist#readme",
  "devDependencies": {
    "cpy-cli": "^1.0.1",
    "http-server": "^0.10.0"
  },
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/polyfill": "7.2.3",
    "@babel/preset-env": "7.2.3",
    "@types/node": "10.12.21",
    "babel-plugin-add-module-exports": "1.0.0",
    "babelify": "10.0.0",
    "browser-resolve": "1.11.3",
    "browserify": "16.2.3",
    "framework7": "4.0.1",
    "framework7-icons": "^0.9.1",
    "moment": "2.24.0",
    "quill": "1.3.7",
    "quill-blot-formatter": "1.0.5",
    "quill-delta-to-html": "0.10.7",
    "scrypt": "^6.0.3",
    "tsify": "4.0.1",
    "typescript": "3.3.1",
    "uglifyify": "5.0.1",
    "watchify": "3.11.0",
    "ipfs-http-client": "30.1.3",
    "ts-node": "8.1.0"
  }
